    "@types/react": "^16.12.0",
    "normalize-scss": "^7.0.0",
    "parcel": "^2.3.1",
    "postcss": "^8.4",
    "postcss-modules": "^4.3.0",
    "postcss-preset-env": "^7.2.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.4.4"
